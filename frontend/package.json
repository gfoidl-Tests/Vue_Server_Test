{
    "name"       : "vue-server-test",
    "version"    : "0.1.0",
    "description": "Test for Vue with Server",
    "author"     : "gfoidl",
    "license"    : "(c) gfoidl, 2020",
    "private"    : true,
    "scripts"    : {
        "build"        : "webpack --mode=development",
        "build-watch"  : "webpack --mode=development --watch",
        "build-release": "webpack --mode=production",
        "start"        : "webpack-dev-server --open chrome --hotOnly",
        "start-server" : "serve ../backend/source/Server/wwwroot --no-clipboard --listen 8080",
        "test"         : "jest",
        "test-unit"    : "jest --projects tests/unit --verbose",
        "test-vue"     : "jest --projects tests/vue --verbose",
        "test-e2e"     : "jest --projects tests/e2e --verbose --reporters=default --reporters=jest-junit",
        "test-coverage": "jest --verbose --reporters=default --reporters=jest-junit --coverage"
    },
    "dependencies": {
        "axios"        : "^0.19.2",
        "bootstrap"    : "^4.4.1",
        "bootstrap-vue": "^2.15.0",
        "vue"          : "^3.0.0-beta.15"
    },
    "devDependencies": {
        "@babel/core"                       : "^7.10.2",
        "@types/expect-puppeteer"           : "^4.4.3",
        "@types/jest"                       : "^26.0.0",
        "@types/jest-environment-puppeteer" : "^4.3.1",
        "@types/jest-image-snapshot"        : "^3.1.0",
        "@types/puppeteer"                  : "^3.0.0",
        "@vue/compiler-sfc"                 : "^3.0.0-beta.15",
        "@vue/test-utils"                   : "^2.0.0-alpha.6",
        "babel-core"                        : "^7.0.0-bridge.0",
        "clean-webpack-plugin"              : "^3.0.0",
        "css-loader"                        : "^3.6.0",
        "file-loader"                       : "^6.0.0",
        "flush-promises"                    : "^1.0.2",
        "fork-ts-checker-webpack-plugin"    : "^5.0.1",
        "git-revision-webpack-plugin"       : "^3.0.6",
        "html-webpack-plugin"               : "^4.2.1",
        "jest"                              : "^26.0.1",
        "jest-image-snapshot"               : "^4.0.2",
        "jest-junit"                        : "^10.0.0",
        "jest-puppeteer"                    : "^4.4.0",
        "less"                              : "^3.11.3",
        "less-loader"                       : "^6.1.1",
        "mini-css-extract-plugin"           : "^0.9.0",
        "mini-svg-data-uri"                 : "^1.1.3",
        "nock"                              : "^12.0.3",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "preload-webpack-plugin"            : "^3.0.0-beta.4",
        "puppeteer"                         : "^2.1.1",
        "serve"                             : "^11.3.2",
        "style-loader"                      : "^1.0.0",
        "terser-webpack-plugin"             : "^3.0.5",
        "ts-jest"                           : "^26.1.0",
        "ts-loader"                         : "^7.0.1",
        "tsconfig-paths"                    : "^3.9.0",
        "tsconfig-paths-webpack-plugin"     : "^3.2.0",
        "tslib"                             : "^2.0.0",
        "typescript"                        : "^3.9.5",
        "url-loader"                        : "^4.1.0",
        "vue-class-component"               : "^8.0.0-alpha.6",
        "vue-jest"                          : "^5.0.0-alpha.0",
        "vue-loader"                        : "^16.0.0-beta.3",
        "vue-property-decorator"            : "^8.5.0",
        "webpack"                           : "^4.41.0",
        "webpack-bundle-analyzer"           : "^3.7.0",
        "webpack-cli"                       : "^3.3.11",
        "webpack-dev-server"                : "^3.8.1"
    }
